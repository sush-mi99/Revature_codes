package frst;

public class AssignOperators {
	
	private int x;
	public AssignOperators(int x) 
	{
		this.x=x; 
	
		switch(x) {
		
		case 1:
			x = x;
		    System.out.println("x =: " + x);
		    break;
		case 2:
			x += x;
		    System.out.println("x +=: " + x);
		    break;
		case 3:
			x *= x;
		    System.out.println("x *=: " + x);
		    break;
		}

	}

}
