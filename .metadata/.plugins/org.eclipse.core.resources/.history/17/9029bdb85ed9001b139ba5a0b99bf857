package frst;

public class AssignOperators {
	
	private int x;
	public AssignOperators(int x) 
	{
		this.x=x; 
		int num = 1;
		switch(x) {
		
		case 1:
			num = x;
		    System.out.println("x =: " + num);
		    break;
		case 2:
			num += x;
		    System.out.println("x +=: " + num);
		    break;
		case 3:
			num *= x;
		    System.out.println("x *=: " + num );
		    break;
		}

	}

}
